          <mj-text mj-class="mjwarning">{{Resources.Emails.TicketClosed_System_Email.Body}}</mj-text>
          <mj-button href="{{ChatUrl}}" mj-class="mj-button">{{Resources.Emails.TicketClosed_System_Email.RateCTA}}</mj-button>
          <mj-text mj-class="mjwarning">{{Resources.Emails.TicketClosed_System_Email.Summary}}</mj-text>
