      <mj-section padding-bottom="24px">
        <mj-column>
          <mj-text font-size="15px">{{Resources.Emails.TicketClosed_System_Email.Header}}</mj-text>
        </mj-column>
      </mj-section>
      <mj-include path="./partials/question.mjml" />
