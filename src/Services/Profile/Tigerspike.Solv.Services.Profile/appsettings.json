{
  "App": {
    "Name": "Profile",
    "Version": "1"
  },
  "Serilog": {
    "Console": {
      "Enabled": true
    },
    "Level": "Warning",
    "AWS": {
      "Enabled": false,
      "LogGroup": "${environment}-Profiling-Container-logs",
      "Region": "eu-west-1"
    },
    "MinimumLevelOverrides": {
      "System": "Error",
      "Microsoft": "Error",
      "Quartz": "Error",
      "Tigerspike.Solv.Infra.Bus": "Error"
    },
    "ExcludePaths": [],
    "ExcludeProperties": [
      "api_key",
      "access_key",
      "ApiKey",
      "ApiSecret",
      "ClientId",
      "ClientSecret",
      "ConnectionString",
      "Password",
      "Email",
      "Login",
      "Secret",
      "Token"
    ]
  },
  "Swagger": {
    "Enabled": true,
    "Name": "v1",
    "Title": "Profile Service",
    "Version": "1.0",
    "RoutePrefix": "swagger",
    "IncludeSecurity": false,
    "AuthorizationUrl": "",
    "OAuthClientId": ""
  },
  "AllowedHosts": "*"
}